@Library('my-lib') _
pipeline {
    agent any
    
    environment {
        repo_link = "https://github.com/HuXn-WebDev/MERN-Movies-App.git"
        branch = "main"
    }
    stages {
        stage("git clone"){
            steps {
                script {
                    gitClone(repo_link, branch)
                }
            }
        }
    }
}